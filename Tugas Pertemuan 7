#include <iostream>

using namespace std;

// definisi class
class MyClass {
    private:
        int x;

    public:
        MyClass(int a) {
            x = a;
        }

        void printX() {
            cout << "Nilai x adalah " << x << endl;
        }
};

int main() {
    // membuat objek dari class
    MyClass obj(5);

    // memanggil method printX()
    obj.printX();

    return 0;
}
